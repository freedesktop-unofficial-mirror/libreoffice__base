#define DIALOG_SIZE_X		220
#define DIALOG_SIZE_Y		140
#define BUTTON_SIZE_X		50
#define BUTTON_SIZE_Y		14

#define LIST_WIDTH			70

#define IL_TOOL \
\
    IdList = {\
        ID_INDEX_NEW;\
        ID_INDEX_DROP;\
        ID_INDEX_RENAME;\
        ID_INDEX_SAVE;\
        ID_INDEX_RESET;\
        };\
    IdCount = {\
    5;\
    }

ImageList IMG_INDEX_DLG_SC
{
    Prefix = "sc";
    MaskColor   = Color {
        Red   = 0xFFFF;
        Green = 0x0000;
        Blue  = 0xFFFF;
    };
    IL_TOOL ;
};
